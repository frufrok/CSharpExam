{
  "Routes": [
    {
      "DownstreamPathTemplate": "/User/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7262
        }
      ],
      "UpstreamPathTemplate": "/User/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post" ],
      "SwaggerKey": "UserAPI"
    },
    {
      "DownstreamPathTemplate": "/Role/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7262
        }
      ],
      "UpstreamPathTemplate": "/Role/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post" ],
      "SwaggerKey": "UserAPI"
    }
  ],

  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:7142/swagger/"
  },

  "SwaggerEndPoints": [
    {
      "Key": "UserAPI",
      "TransformByOcelogConfig": false,
      "Config": [
        {
          "Name": "Users API",
          "Version": "v1",
          "Url": "https://localhost:7262/swagger/v1/swagger.json"
        }
      ]
    }
  ]

}
